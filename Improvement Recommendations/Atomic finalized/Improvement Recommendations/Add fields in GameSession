// in state.rs
#[derive(AnchorSerialize, AnchorDeserialize, Clone, PartialEq, Eq)]
pub enum SessionState {
    Open = 0,
    Finalized = 1,
}
pub struct GameSession {
    pub authority: Pubkey,
    pub session_id: [u8; 32],
    pub state: u8, // 0 = Open, 1 = Finalized (or use enum via u8)
    // ...
}
